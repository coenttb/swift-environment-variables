# Example environment file in KEY=VALUE format
# This file demonstrates the supported .env format with consistent keys

# Application info
APP_NAME=Swift Environment Variables
APP_VERSION=1.0.0
APP_ENV=keyvalue-demo
DEBUG=true

# Server configuration
SERVER_HOST=localhost
SERVER_PORT=4000
BASE_URL=https://keyvalue.example.com

# Database configuration
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=myapp_keyvalue
DATABASE_URL=postgresql://kv:password@localhost:5432/myapp_keyvalue

# Cache and Redis
CACHE_TTL=900
REDIS_URL=redis://localhost:6379/3

# Logging
LOG_LEVEL=debug
ENABLE_LOGGING=true

# Feature flags
FEATURE_FLAG_NEW_UI=false
FEATURE_FLAG_BETA_FEATURES=true
FEATURE_FLAG_ANALYTICS=false

# API and security
API_KEY=keyvalue-demo-api-key
SECRET_KEY=keyvalue-demo-secret-key
JWT_SECRET=keyvalue-demo-jwt-secret

# External services
SMTP_HOST=smtp.keyvalue.com
SMTP_PORT=587
SMTP_FROM=demo@keyvalue.com

# Performance and limits
CONNECTION_POOL_SIZE=8
RATE_LIMIT_MAX=200
REQUEST_TIMEOUT=25

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9092
HEALTH_CHECK_ENABLED=true

# Values with spaces (quoted)
COMPANY_NAME="Coen ten Thije Boonkkamp"
DESCRIPTION="A Swift environment variables package"

# Values with single quotes
API_VERSION='v2.0'
ENVIRONMENT='keyvalue-demo'

# Empty value
EMPTY_VAR=

# Value with special characters (quoted to preserve # symbol)
SPECIAL_CHARS="key@#$%^&*()"

# Values that look like other formats
JSON_LIKE={"key":"value"}
ARRAY_LIKE=[1,2,3]

# Type conversion test values
STRING=hello_world
BOOL=true
PORT=8080
URL=https://coenttb.com
NUMBER=42
FLOAT=3.14159